{
  "name": "monopro-ai-cli",
  "version": "0.2.0",
  "description": "CLI for Monopro AI",
  "main": "dist/index.js",
  "keywords": [
    "ai",
    "metrics",
    "test",
    "measure",
    "ml",
    "monopro",
    "monopro-ai",
    "cli"
  ],
  "homepage": "https://github.com/mariesco/monopro-ai",
  "bugs": {
    "url": "https://github.com/mariesco/monopro-ai/issues"
  },
  "author": "Mart√≠n Riesco <martinriesco16@gmail.com> (https://github.com/mariesco)",
  "bin": {
    "monopro": "./dist/bin/run.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mariesco/monopro-ai.git"
  },
  "files": [
    "dist",
    "drizzle_migrations"
  ],
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "tsc && npm run format",
    "ci": "npm run build && npm run check-format && npm run check-exports && npm run test",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "check-exports": "attw --pack . --ignore-rules cjs-resolves-to-esm",
    "test": "vitest run",
    "dev": "vitest",
    "local-release": "changeset version && changeset publish",
    "prepublishOnly": "npm run ci"
  },
  "dependencies": {
    "@inquirer/prompts": "^5.3.8",
    "@oclif/core": "^4.0.19",
    "dotenv": "^16.4.5",
    "monopro-ai": "*"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.15.4",
    "@changesets/cli": "^2.27.7",
    "@types/node": "^18.19.47",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  },
  "types": "./dist/index.d.ts",
  "oclif": {
    "bin": "monopro",
    "commands": "./dist/commands",
    "dirname": "monopro",
    "topicSeparator": " "
  }
}
